<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<import
		resource="classpath:/WEB-INF/population/conversion-service.xml" />

	<!-- Shouts -->
	
	<bean id="shout01" class="acme.entities.shouts.Shout">
		<property name="moment" value="2021/02/02 19:39" />
		<property name="author" value="John Doe" />
		<property name="text" value="I'm happy!" />
		<property name="info"><null /></property>
	</bean>

	<bean id="shout02" class="acme.entities.shouts.Shout">
		<property name="moment" value="2021/02/02 15:55" />
		<property name="author" value="Jane Doe" />
		<property name="text" value="I'm the master of my room!" />
		<property name="info" value="http://www.example.org" />
	</bean>

	<bean id="shout03" class="acme.entities.shouts.Shout">
		<property name="moment" value="2021/02/02 00:00" />
		<property name="author" value="Foo Bar" />
		<property name="text" value="Just turning 50!" />
		<property name="info" value="http://happybirthday.com" />
	</bean>

	<bean id="shout04" class="acme.entities.shouts.Shout">
		<property name="author" value="Leslie Design-Testing" />
		<property name="text" value="Woa! This app's so cool!" />
		<property name="moment" value="2021/02/02 12:30" />
		<property name="info" value="http://www.sample-app.com" />
	</bean>

	<!-- Tasks -->
	
	<bean id="task01" class="acme.framework.entities.Task">
		<property name="title" value="Task number one" />
		<property name="description" value="A public task which is the first one" />
		<property name="link" value="http://first.task" />
		<property name="isPrivate" value="false" />
		<property name="executionStart" value="2022/02/01 14:00" />
		<property name="executionEnd" value="2022/03/25 17:30" />
		<property name="workload" value="1.5" />
	</bean>

	<bean id="task02" class="acme.framework.entities.Task">
		<property name="title" value="Task number two" />
		<property name="description" value="A private task which is the second one" />
		<property name="link" value="http://second.task" />
		<property name="isPrivate" value="true" />
		<property name="executionStart" value="2021/04/20 11:00" />
		<property name="executionEnd" value="2021/12/02 20:00" />
		<property name="workload" value="8.3" />
	</bean>

	<bean id="task03" class="acme.framework.entities.Task">
		<property name="title" value="Task number three" />
		<property name="description" value="A public task which is the third one" />
		<property name="link" value="http://third.task" />
		<property name="isPrivate" value="false" />
		<property name="executionStart" value="2021/09/20 10:00" />
		<property name="executionEnd" value="2021/11/02 12:00" />
		<property name="workload" value="1.9" />
	</bean>

	<bean id="task04" class="acme.framework.entities.Task">
		<property name="title" value="Task number four" />
		<property name="description" value="A private task which is the fourth one" />
		<property name="link" value="http://fourth.task" />
		<property name="isPrivate" value="true" />
		<property name="executionStart" value="2022/04/09 10:00" />
		<property name="executionEnd" value="2022/12/23 18:00" />
		<property name="workload" value="8.8" />
	</bean>

</beans>
